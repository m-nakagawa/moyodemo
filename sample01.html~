  <script src="http://code.jquery.com/jquery-1.11.3.min.js"></script>
  <script src="sample01.js" charset="utf-8"></script>
  <script type="text/javascript">
    path1 ='ws://localhost:3030/openmasami/sample01/read/path/device/value?-latest=1&-query='
    query = 'SELECT ?hub WHERE {  ?s0 <http://bizar.aitc.jp/ns/fos/0.1/tag> <http://bizar.aitc.jp/ns/fos/0.1/local/label#device> .  ?s0 <http://bizar.aitc.jp/ns/fos/0.1/local/label#value> ?hub .}';
    labelns = 'http://bizar.aitc.jp/ns/fos/0.1/local/label#'
    path1 = (path1 + encodeURIComponent(query)
	  +"&-propertyns="
	  +encodeURIComponent(labelns));
    console.log("Start");
    connect1(path1);
  </script>
  <table>
